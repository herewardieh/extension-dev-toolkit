{
  "name": "@extension-dev-packages/extension-cli",
  "version": "0.0.1",
  "packBundle": {
    "rollupReplace": {
      "process.env.APP_VERSION": "0.0.1"
    },
    "sideEffects": [
      "@extension-dev-packages/types-extension-manifest",
      "@types/cors",
      "@types/express",
      "@types/inquirer",
      "@types/fs-extra"
    ]
  },
  "scripts": {
    "prepublishOnly": "../../node_modules/.bin/pack-bundle"
  },
  "keywords": [
    "chrome-extension",
    "tool-chain",
    "command",
    "vite"
  ],
  "author": {
    "name": "seasonstorm@icloud.com"
  },
  "bin": {
    "extension-cli": "./bin/index.mjs"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./index.ts": "./index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herewardieh/extension-dev-toolkit.git"
  },
  "dependencies": {
    "tslib": "^2.6.2",
    "commander": "^12.0.0",
    "jju": "^1.4.0",
    "path-parse": "^1.0.7",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "jszip": "^3.10.1",
    "rimraf": "^5.0.5",
    "inquirer": "^9.2.22",
    "tempy": "^3.1.0",
    "fs-extra": "^11.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "vite": "^5.2.11",
    "eventemitter3": "^5.0.1",
    "chokidar": "^3.6.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "chalk": "^5.3.0",
    "move-file": "^3.1.0",
    "@extension-dev-packages/types-extension-manifest": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4"
  },
  "_id": "@extension-dev-packages/extension-cli@0.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "bugs": {
    "url": "https://github.com/herewardieh/extension-dev-toolkit/issues"
  },
  "homepage": "https://github.com/herewardieh/extension-dev-toolkit#readme"
}